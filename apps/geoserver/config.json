{
    "name": "GeoServer",
    "id": "geoserver",
    "available": true,
    "short_desc": "The reference OGC server for WFS/WMS editing.",
    "author": "Eric D",
    "categories": [
        "utilities",
        "data"
    ],
    "port": 8080,
    "version": "latest",
    "tipi_version": 2,
    "description": "Publish and share geospatial data using open standards (OGC). Connects to PostGIS to serve WMS (Maps), WFS (Features), and WCS (Coverages).",
    "source": "http://geoserver.org/",
    "exposable": true,
    "supported_architectures": [
        "amd64",
        "arm64"
    ],
    "form_fields": [
        {
            "type": "text",
            "env_variable": "GEOSERVER_ADMIN_USER",
            "label": "Admin User",
            "default": "admin",
            "required": true
        },
        {
            "type": "password",
            "env_variable": "GEOSERVER_ADMIN_PASSWORD",
            "label": "Admin Password",
            "required": true
        }
    ]
}