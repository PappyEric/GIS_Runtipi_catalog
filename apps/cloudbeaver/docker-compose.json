{
    "services": [
        {
            "name": "cloudbeaver",
            "image": "dbeaver/cloudbeaver:latest",
            "isMain": true,
            "internalPort": "8978",
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/workspace",
                    "containerPath": "/opt/cloudbeaver/workspace"
                }
            ]
        }
    ]
}