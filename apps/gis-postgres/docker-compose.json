{
    "services": [
        {
            "name": "gis-postgres",
            "image": "postgis/postgis:16-3.4",
            "isMain": true,
            "internalPort": "5432",
            "environment": {
                "POSTGRES_USER": "${POSTGRES_USER}",
                "POSTGRES_PASSWORD": "${POSTGRES_PASSWORD}",
                "POSTGRES_DB": "${POSTGRES_DB}"
            },
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/data",
                    "containerPath": "/var/lib/postgresql/data"
                }
            ]
        }
    ]
}