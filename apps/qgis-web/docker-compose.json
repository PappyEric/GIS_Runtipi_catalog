{
    "services": [
        {
            "name": "qgis-web",
            "image": "lscr.io/linuxserver/qgis:latest",
            "isMain": true,
            "internalPort": "3000",
            "environment": {
                "PGID": "1000",
                "TZ": "Etc/UTC",
                "CUSTOM_USER": "${KASM_USER}",
                "PASSWORD": "${KASM_PASSWORD}"
            },
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/config",
                    "containerPath": "/config"
                }
            ]
        }
    ]
}