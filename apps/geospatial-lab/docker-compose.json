{
    "services": [
        {
            "name": "geospatial-lab",
            "image": "quay.io/jupyter/datascience-notebook:latest",
            "isMain": true,
            "internalPort": "8888",
            "environment": {
                "JUPYTER_TOKEN": "${APP_PASSWORD}",
                "JUPYTER_ENABLE_LAB": "yes"
            },
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/work",
                    "containerPath": "/home/jovyan/work"
                }
            ],
            "command": "start-notebook.sh --NotebookApp.token='${APP_PASSWORD}'"
        }
    ]
}