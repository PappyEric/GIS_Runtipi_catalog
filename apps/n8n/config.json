{
    "name": "n8n Automation",
    "id": "n8n",
    "available": true,
    "short_desc": "Workflow automation tool.",
    "author": "Eric D",
    "categories": [
        "utilities",
        "development"
    ],
    "port": 5678,
    "version": "latest",
    "tipi_version": 2,
    "description": "n8n allows you to build complex workflows. Use it to connect Mergin Maps to Postgres, or trigger AI analysis.",
    "source": "https://n8n.io",
    "exposable": true,
    "supported_architectures": [
        "amd64",
        "arm64"
    ],
    "form_fields": [
        {
            "type": "text",
            "env_variable": "DB_POSTGRESDB_HOST",
            "label": "Postgres Host",
            "default": "gis-postgres",
            "required": true
        },
        {
            "type": "text",
            "env_variable": "DB_POSTGRESDB_USER",
            "label": "Postgres User",
            "default": "tipi",
            "required": true
        },
        {
            "type": "password",
            "env_variable": "DB_POSTGRESDB_PASSWORD",
            "label": "Postgres Password",
            "required": true
        },
        {
            "type": "text",
            "env_variable": "DB_POSTGRESDB_DATABASE",
            "label": "Postgres Database",
            "default": "gis",
            "required": true
        }
    ]
}