{
    "services": [
        {
            "name": "potree",
            "image": "nginx:alpine",
            "isMain": true,
            "internalPort": "80",
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/www",
                    "containerPath": "/usr/share/nginx/html"
                }
            ]
        }
    ]
}